{
    "name": "tinfoil-electron",
    "version": "1.0.0",
    "description": "FLAC Audio Fingerprinting and Metadata Management",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dev": "concurrently \"webpack --watch\" \"npm start\"",
        "build": "webpack --mode production && electron-builder",
        "build-css": "tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css",
        "watch-css": "tailwindcss -i ./src/styles/input.css -o ./src/styles/output.css --watch"
    },
    "dependencies": {
        "electron-is-dev": "^2.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.26.9",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-react": "^7.26.3",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^9.2.1",
        "concurrently": "^8.0.1",
        "css-loader": "^6.11.0",
        "electron": "^24.1.3",
        "electron-builder": "^23.6.0",
        "postcss": "^8.4.23",
        "postcss-loader": "^7.3.4",
        "style-loader": "^3.3.4",
        "tailwindcss": "^3.3.2",
        "webpack": "^5.98.0",
        "webpack-cli": "^5.1.4"
    }
}
