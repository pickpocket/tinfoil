{
  "name": "tinfoil",
  "version": "1.0.0",
  "description": "FLAC Audio Fingerprinting and Metadata Management",
  "main": "main.js",
  "author": "Treverm",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0"
  },
  "devDependencies": {
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0"
  },
  "build": {
    "appId": "com.tinfoil.app",
    "productName": "Tinfoil",
    "files": [
      "main.js",
      "preload.js",
      "public/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "python",
        "to": "python",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}